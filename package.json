{
  "name": "@bruhabruh/ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "description": "BruhaBruh UI Library",
  "author": "BruhaBruh",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bruhabruh/ui"
  },
  "scripts": {
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "serve-storybook": "serve storybook-static",
    "theme": "pnpm run theme:css && pnpm run theme:tailwind",
    "theme:css": "pnpm dlx @bruhabruh/theme@1.1.0 css -o ./src/theme.css",
    "theme:tailwind": "pnpm dlx @bruhabruh/theme@1.1.0 tailwind -o ./.generated/tailwind.json",
    "prettier": "prettier '**/*.{md,yaml,yml,json}'",
    "prepare": "husky"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@bruhabruh/eslint-config": "^1.0.0",
    "@bruhabruh/prettier-config": "^1.1.0",
    "@bruhabruh/tsconfig": "^1.0.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/types": "^19.0.3",
    "@storybook/addon-a11y": "^8.1.9",
    "@storybook/addon-controls": "^8.1.9",
    "@storybook/addon-docs": "^8.1.9",
    "@storybook/addon-measure": "^8.1.9",
    "@storybook/addon-outline": "^8.1.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-themes": "^8.1.9",
    "@storybook/addon-toolbars": "^8.1.9",
    "@storybook/addon-viewport": "^8.1.9",
    "@storybook/blocks": "^8.1.9",
    "@storybook/react": "^8.1.9",
    "@storybook/react-vite": "^8.1.9",
    "@storybook/test": "^8.1.9",
    "@storybook/types": "^8.1.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "nano-staged": "^0.8.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "storybook": "^8.1.11",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^1.6.0"
  },
  "nano-staged": {
    "*.{js,ts}": [
      "eslint --fix --quiet --report-unused-disable-directives"
    ],
    "*.{md,yaml,yml,json}": [
      "prettier --write"
    ]
  }
}
