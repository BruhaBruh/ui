{
  "name": "@bruhabruh/ui",
  "version": "2.0.0",
  "type": "module",
  "description": "BruhaBruh UI Library",
  "author": "BruhaBruh",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bruhabruh/ui"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite.config.docs.ts",
    "generate": "pnpm dlx @bruhabruh/theme@3.6.1 generate",
    "build": "tsc && vite build",
    "build:docs": "vite build --config vite.config.docs.ts",
    "preview": "vite preview --config vite.config.docs.ts",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prettier": "prettier '**/*.{md,yaml,yml,json}'",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@internationalized/date": "^3.5.6",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-aria/toast": "3.0.0-beta.17",
    "@react-stately/toast": "3.0.0-beta.6",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "fast-equals": "^5.0.1",
    "motion": "^11.11.17",
    "react": "^18.3.1",
    "react-aria": "^3.35.1",
    "react-dom": "^18.3.1",
    "react-stately": "^3.33.0",
    "tailwind-merge": "^2.5.4"
  },
  "devDependencies": {
    "@bruhabruh/eslint-config": "^1.3.0",
    "@bruhabruh/prettier-config": "^1.1.3",
    "@bruhabruh/theme": "^3.6.1",
    "@bruhabruh/tsconfig": "^1.0.2",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@commitlint/types": "^19.5.0",
    "@eslint/markdown": "^6.2.2",
    "@tabler/icons-react": "^3.29.0",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.6",
    "eslint": "^9.19.0",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "postcss": "^8.5.1",
    "postcss-hover-media-feature": "^1.0.2",
    "prettier": "^3.4.2",
    "react-router": "^7.1.5",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.5"
  },
  "overrides": {
    "eslint": "^9.14"
  },
  "nano-staged": {
    "*.{js,ts,md,json}": [
      "eslint --fix --quiet --report-unused-disable-directives"
    ],
    "*.{md,yaml,yml,json}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@bruhabruh/type-safe": "^1.2.0"
  }
}
