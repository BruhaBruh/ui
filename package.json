{
  "name": "@bruhabruh/ui",
  "version": "2.0.2",
  "type": "module",
  "description": "BruhaBruh UI Library",
  "author": "BruhaBruh",
  "license": "MIT",
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/bruhabruh/ui"
  },
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "vite --config vite.config.docs.ts",
    "generate": "pnpm dlx @bruhabruh/theme@latest generate",
    "generate:registry": "tsx ./src/cli/generate-registry.ts",
    "build": "tsc && vite build",
    "build:cli": "tsup",
    "build:docs": "vite build --config vite.config.docs.ts",
    "preview": "vite preview --config vite.config.docs.ts",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "prettier": "prettier '**/*.{md,yaml,yml,json}'",
    "prepare": "husky"
  },
  "dependencies": {
    "@antfu/ni": "^23.3.1",
    "@radix-ui/react-slot": "^1.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^13.1.0",
    "fast-equals": "^5.2.2",
    "kleur": "^4.1.5",
    "motion": "^12.4.10",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "react": "^19.0.0",
    "react-aria": "^3.38.0",
    "react-dom": "^19.0.0",
    "react-scan": "^0.2.13",
    "react-stately": "^3.36.0",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@bruhabruh/eslint-config": "^1.3.1",
    "@bruhabruh/prettier-config": "^1.1.4",
    "@bruhabruh/theme": "^3.6.2",
    "@bruhabruh/tsconfig": "^1.0.4",
    "@bruhabruh/type-safe": "^1.2.1",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@commitlint/types": "^19.8.0",
    "@eslint/markdown": "^6.3.0",
    "@tabler/icons-react": "^3.31.0",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.0.6",
    "eslint": "^9.22.0",
    "glob": "^11.0.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "postcss": "^8.5.3",
    "postcss-hover-media-feature": "^1.0.2",
    "prettier": "^3.5.3",
    "react-router": "^7.3.0",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vite-plugin-dts": "^4.5.3",
    "vitest": "^3.0.8"
  },
  "overrides": {
    "eslint": "^9.14"
  },
  "nano-staged": {
    "*.{js,ts,md,json}": [
      "eslint --fix --quiet --report-unused-disable-directives"
    ],
    "*.{md,yaml,yml,json}": [
      "prettier --write"
    ]
  }
}
